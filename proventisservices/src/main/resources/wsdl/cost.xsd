<?xml version="1.0" encoding="ISO-8859-1"?>
<xsd:schema targetNamespace="http://cost.blueant.axis.proventis.net/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:base="http://base.blueant.axis.proventis.net/"
	xmlns:cost="http://cost.blueant.axis.proventis.net/"
	elementFormDefault="qualified" xmlns:ticket="http://ticket.blueant.axis.proventis.net/">

	<xsd:import schemaLocation="base.xsd"
		namespace="http://base.blueant.axis.proventis.net/" />

	<xsd:import schemaLocation="ticket.xsd" namespace="http://ticket.blueant.axis.proventis.net/"></xsd:import>
	<xsd:element name="SimpleProject" type="cost:T_SimpleProject" />

	<xsd:complexType name="T_SimpleProject">
		<xsd:annotation>
			<xsd:documentation>a single project</xsd:documentation>

		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="projectID" type="base:id"
				nillable="true">
				<xsd:annotation>
					<xsd:documentation>
						Projekt-ID - Beim Anlegen eines Projektes nicht
						gesetzt - lesend oder ändernd
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="name" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Name des Projektes
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="startTime" type="xsd:dateTime">
				<xsd:annotation>

					<xsd:documentation>Projektstart</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="endTime" type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation>Projektende</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="SimpleTask" type="cost:T_SimpleTask" />

	<xsd:complexType name="T_SimpleTask">
		<xsd:sequence>
			<xsd:element name="taskID" type="base:id">
				<xsd:annotation>
					<xsd:documentation>PrimaryKey</xsd:documentation>

				</xsd:annotation>
			</xsd:element>
			<xsd:element name="name" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Bezeichnung der Aktivität
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="startTime" type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation>
						Beginn der Aktivität
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="endTime" type="xsd:dateTime">
				<xsd:annotation>

					<xsd:documentation>
						Ende der Aktivität
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="getPersonalWorktimeRequestParameter"
		type="cost:T_PersonalWorktimeRequestParameter" />

	<xsd:complexType name="T_PersonalWorktimeRequestParameter">
		<xsd:sequence>
			<xsd:element name="sessionID" type="xsd:string" />
			<xsd:element name="workTimeID" type="base:id"
				nillable="true">
				<xsd:annotation>
					<xsd:documentation>
						Nur den Datensatz mit dieser ID holen
					</xsd:documentation>
				</xsd:annotation>

			</xsd:element>
			<xsd:element name="fromDate" type="xsd:date"
				nillable="true">
				<xsd:annotation>
					<xsd:documentation>
						Nur den Datensätz holen, die nach fromDate
						liegen
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="toDate" type="xsd:date"
				nillable="true">

				<xsd:annotation>
					<xsd:documentation>
						Nur den Datensätz holen, die vor toDate liegen
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ticketID" type="base:id"
				nillable="true">
				<xsd:annotation>
					<xsd:documentation>

						Nur den Datensätz holen, die zu diesem Ticket
						gehören
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="projectID" type="base:id"
				nillable="true">
				<xsd:annotation>
					<xsd:documentation>
						Nur den Datensätz holen, die zu diesem Projekt
						gehören
					</xsd:documentation>
				</xsd:annotation>

			</xsd:element>
			<xsd:element name="taskID" type="base:id" nillable="true">
				<xsd:annotation>
					<xsd:documentation>
						Nur den Datensätz holen, die zu dieser Aktivität
						gehören
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="state" type="base:optionalInt"
				nillable="true">

				<xsd:annotation>
					<xsd:documentation>
						Nur den Datensätz holen, die in diesem Status
						sind
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="billable" type="base:optionalBoolean"
				nillable="true">
				<xsd:annotation>
					<xsd:documentation>

						Nur den Datensätz holen, die abrechenbar sind
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="getWorktimeRequestParameter">
		<xsd:complexType>
			<xsd:complexContent>

				<xsd:extension
					base="cost:T_PersonalWorktimeRequestParameter">
					<xsd:sequence>
						<xsd:element name="personNumber"
							type="xsd:string" nillable="true">
							<xsd:annotation>
								<xsd:documentation>
									Nur den Datensätz holen, die für
									Person(en) mit dieser/n
									Personalnummer(n) erfaßt wurden
									(Like-Suche)
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<xsd:element name="personID" type="base:id"
							nillable="true">
							<xsd:annotation>
								<xsd:documentation>
									Nur den Datensätz holen, die für die
									Person mit dieser PersonalID erfaßt
									wurden
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="fistname" type="xsd:string"
							nillable="true">
							<xsd:annotation>

								<xsd:documentation>
									Nur den Datensätz holen, die für
									Personen mit diesem Vornamen erfaßt
									wurden (Like-Suche)
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="lastname" type="xsd:string"
							nillable="true">
							<xsd:annotation>
								<xsd:documentation>
									Nur den Datensätz holen, die für
									Personen mit diesem Nachnamen erfaßt
									wurden (Like-Suche)
								</xsd:documentation>

							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="editWorktimeRequestParameter"
		type="cost:T_EditWorktimeRequestParameter" />

	<xsd:complexType name="T_EditWorktimeRequestParameter">
		<xsd:sequence>
			<xsd:element name="sessionID" type="xsd:string" />
			<xsd:element name="workTimeID" type="base:id"
				nillable="true">
				<xsd:annotation>
					<xsd:documentation>
						Bei Neuanlage nicht angeben
					</xsd:documentation>
				</xsd:annotation>

			</xsd:element>
			<xsd:element name="date" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Datum der AZ-Erfassung (Format in der
						SOAP-Nachticht: hh:mm:ss.000Z)
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="from" type="xsd:time" nillable="true">

				<xsd:annotation>
					<xsd:documentation>
						StartZeitpunkt (Format in der SOAP-Nachticht:
						hh:mm:ss.000Z)
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="to" type="xsd:time" nillable="true">
				<xsd:annotation>
					<xsd:documentation>Endzeitpunkt</xsd:documentation>

				</xsd:annotation>
			</xsd:element>
			<xsd:element name="break-share" type="xsd:double"
				nillable="true">
				<xsd:annotation>
					<xsd:documentation>
						Pausendauer in ms (teilbar durch 60000)
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="duration" type="xsd:double"
				nillable="true">
				<xsd:annotation>
					<xsd:documentation>
						Wenn in Blue Ant die Dauerfassung eingestellt
						ist, dann lediglich die Dauer in Sekunden, sonst
						von/bis
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ticketID" type="base:id"
				nillable="true">
				<xsd:annotation>

					<xsd:documentation>Ticket</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="projectID" type="base:id"
				nillable="true">
				<xsd:annotation>
					<xsd:documentation>Projekt</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="taskID" type="base:id" nillable="true">
				<xsd:annotation>
					<xsd:documentation>
						AktivitätenID aus der PrjPlanung
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="activityID" type="base:id">
				<xsd:annotation>

					<xsd:documentation>
						TätigkeitenID aus den Stammdaten
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="comment" type="xsd:string"
				nillable="true">
				<xsd:annotation>
					<xsd:documentation>
						Kommentar (max. 100 oder 1000 Zeichen, je nach
						Customizingeinstellung)
					</xsd:documentation>

				</xsd:annotation>
			</xsd:element>
			<xsd:element name="billable" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>
						AZ fakturierbar
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="iccID" type="base:id" nillable="true">
				<xsd:annotation>
					<xsd:documentation>
						Hilfskostenstelle
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="WorkTime" type="cost:T_WorkTime" />

	<xsd:complexType name="T_WorkTime">
		<xsd:complexContent>
			<xsd:extension base="cost:T_EditWorktimeRequestParameter">
				<xsd:sequence>
					<xsd:element name="state" type="xsd:int">
						<xsd:annotation>
							<xsd:documentation>

								Status der einzelnen AZ.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="personID" type="base:id">
						<xsd:annotation>
							<xsd:documentation>
								Kennung des Mitarbeiters, der die AZ
								erfasst hat.
							</xsd:documentation>
						</xsd:annotation>

					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:element name="WorktimeList">
		<xsd:complexType>
			<xsd:sequence>

				<xsd:element maxOccurs="unbounded" ref="cost:WorkTime" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="deleteWorktimeRequestParameter">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="sessionID" type="xsd:string" />

				<xsd:element name="workTimeID" type="base:id">
					<xsd:annotation>
						<xsd:documentation>
							Die ID des zu löschenden Datensatzes
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>

	</xsd:element>

	<xsd:simpleType name="WorkTimeStateValues">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="0">
				<xsd:annotation>
					<xsd:documentation>Offen</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>

			<xsd:enumeration value="1">
				<xsd:annotation>
					<xsd:documentation>
						Bestätigt/Freigegeben
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="2">
				<xsd:annotation>

					<xsd:documentation>
						Fremdsystem/Gesperrt
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:element name="WorkTimeState" type="cost:T_WorkTimeState" />

	<xsd:complexType name="T_WorkTimeState">
		<xsd:sequence>
			<xsd:element name="workTimeID" type="base:id">
				<xsd:annotation>
					<xsd:documentation>
						PrimaryKey der Arbeitszeit
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="state" type="cost:WorkTimeStateValues">
				<xsd:annotation>
					<xsd:documentation>
						Status der Arbeitszeit (offen, freigegeben,
						gesperrt)
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="WorkTimeStateList"
		type="cost:T_WorkTimeStateList" />

	<xsd:complexType name="T_WorkTimeStateList">
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" ref="cost:WorkTimeState" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="WorkTimeStateResponse"
		type="cost:T_WorkTimeStateResponse" />

	<xsd:complexType name="T_WorkTimeStateResponse">
		<xsd:sequence>
			<xsd:element name="workTimeID" type="base:id" />
			<xsd:element name="message" nillable="true"
				type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Message leer wenn OK, sonst Fehlermeldung
					</xsd:documentation>
				</xsd:annotation>

			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="WorkTimeStateResponseList">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element maxOccurs="unbounded"
					ref="cost:WorkTimeStateResponse" />
			</xsd:sequence>

		</xsd:complexType>
	</xsd:element>

	<xsd:element name="changeWorktimeStateRequestParameter">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="sessionID" type="xsd:string"/>
				<xsd:element name="worktimeStateList" type="cost:T_WorkTimeStateList"/>
			</xsd:sequence>

		</xsd:complexType>
	</xsd:element>

	<xsd:element name="setEstimatedPlanRequestParameter">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="sessionID" type="xsd:string" />
				<xsd:element name="taskID" type="base:id" />
				<xsd:element name="estimatedPlan" type="xsd:double">

					<xsd:annotation>
						<xsd:documentation>
							Der geschätzte Mehraufwand in Minuten
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:simpleType name="T_ProjectStateValue">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="open">
				<xsd:annotation>
					<xsd:documentation>
						Projekte mit offene Aktivitäten zur
						AZ-Erfassung. Die Customizing-Einstellung "Wenn
						Aktivität fertig, dann AZ- und
						Fortschritterfassung für MA sperren?" wird
						berücksichtigt.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>

			<xsd:enumeration value="all">
				<xsd:annotation>
					<xsd:documentation>
						Alle Projekte (default)
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="incomplete">
				<xsd:annotation>

					<xsd:documentation>
						Projekte mit Aktivitäten mit einer
						Fertigstellung unter 100%. Die
						Customizing-Einstellung "Wenn Aktivität fertig,
						dann AZ- und Fortschritterfassung für MA
						sperren?" wird nicht berücksichtigt.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:element name="getProjectsRequestParameter">
		<xsd:complexType>

			<xsd:sequence>
				<xsd:element name="sessionID" type="xsd:string" />
				<xsd:element name="selectedProjectID" type="base:id"
					nillable="true">
					<xsd:annotation>
						<xsd:documentation>
							Die Ergebnismenge wird um einen
							Projekteintrag erweitert, wenn die akt. zu
							bearbeitende AZ einem Projekt zugeqwiesen
							ist,welches nicht mehr sichtbar für die
							AZ-Erfassung ist.
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<xsd:element name="projectState"
					type="cost:T_ProjectStateValue" nillable="true">
					<xsd:annotation>
						<xsd:documentation>
							Welche Projekte sollen geliefert werden ?
							(offen, alle, nicht-fertig)
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>

	</xsd:element>

	<xsd:element name="WorkTimeProject" type="cost:T_WorkTimeProject" />

	<xsd:complexType name="T_WorkTimeProject">
		<xsd:complexContent>
			<xsd:extension base="cost:T_SimpleProject">
				<xsd:sequence>
					<xsd:element name="billable" type="xsd:boolean">
						<xsd:annotation>

							<xsd:documentation>
								Das Projekt ist fakturierbar
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taskCompulsory"
						type="xsd:boolean">
						<xsd:annotation>
							<xsd:documentation>
								Aktivität ist Pflichtfeld
							</xsd:documentation>

						</xsd:annotation>
					</xsd:element>
					<xsd:element name="commentCompulsory"
						type="xsd:boolean">
						<xsd:annotation>
							<xsd:documentation>
								Bememerkung ist Pflichtfeld
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:element name="WorkTimeProjectList">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element minOccurs="0" maxOccurs="unbounded"
					ref="cost:WorkTimeProject" />

			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="getTasksRequestParameter">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="sessionID" type="xsd:string" />
				<xsd:element name="projectID" type="base:id" />

				<xsd:element name="selectedTaskID" type="base:id"
					nillable="true">
					<xsd:annotation>
						<xsd:documentation>
							Die Ergebnismenge wird um einen
							Aktivitäteneintrag erweitert, wenn die akt.
							zu bearbeitende AZ einer Aktivität
							zugewiesen ist, welche nicht mehr sichtbar
							für die AZ-Erfassung ist.
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>

	</xsd:element>

	<xsd:element name="WorkTimeTask" type="cost:T_WorkTimeTask" />

	<xsd:complexType name="T_WorkTimeTask">
		<xsd:annotation>
			<xsd:documentation>
				Die laufende Nummer eines Task in Abhängigkeit der
				Projekt-Einstellung
			</xsd:documentation>
		</xsd:annotation>

		<xsd:complexContent>
			<xsd:extension base="cost:T_SimpleTask">
				<xsd:sequence>
					<xsd:element name="number" type="xsd:string"></xsd:element>
					<xsd:element name="subjectiveProgressAll"
						type="xsd:double" />
					<xsd:element name="subjectiveProgress"
						type="xsd:double" />
					<xsd:element name="plannedTimeAll"
						type="xsd:double" />
					<xsd:element name="plannedTime" type="xsd:double" />
					<xsd:element name="accountedTimeAll"
						type="xsd:double" />

					<xsd:element name="accountedTime" type="xsd:double" />
					<xsd:element name="worktimeAllowed"
						type="xsd:boolean" />
					<xsd:element name="children" nillable="true"
						type="cost:T_WorkTimeTaskList" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:element name="WorkTimeTaskList" type="cost:T_WorkTimeTaskList" />

	<xsd:complexType name="T_WorkTimeTaskList">
		<xsd:sequence>
			<xsd:element minOccurs="0" maxOccurs="unbounded"
				ref="cost:WorkTimeTask" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="setSubjectiveProgressRequestParameter">
		<xsd:complexType>
			<xsd:sequence>

				<xsd:element name="sessionID" type="xsd:string" />
				<xsd:element name="taskID" type="base:id" />
				<xsd:element name="subjectiveProgress"
					type="xsd:double">
					<xsd:annotation>
						<xsd:documentation>
							Der subjektive Fortschritt. Die möglichen
							Werte sin von der Einstellung Erfassung
							Fertigstellung Projektleiter/Mitarbeiter" im
							Menupunkt
							Administration-Customizing-Projekte-Allgemein
							abhängig. Listbox - Es sind nur Werte aus
							MasterData.getProgressList erlaubt. Eingabe
							- Es sind alle ganzzahligen Werte zwische 0
							und 100 (inklusive) erlaubt erlaubt.
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>


	<xsd:element name="getWorktimeTicketsRequestParameter">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="sessionID" type="xsd:string"></xsd:element>
			</xsd:sequence>

		</xsd:complexType>
	</xsd:element>

	<xsd:element name="WorkTimeTicket" type="cost:T_WorkTimeTicket"></xsd:element>

	<xsd:element name="WorkTimeTicketList"
		type="cost:T_WorkTimeTicketList">
	</xsd:element>

	<xsd:complexType name="T_WorkTimeTicket">
		<xsd:complexContent>

			<xsd:extension base="ticket:T_Ticket"></xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="T_WorkTimeTicketList">
		<xsd:sequence>
			<xsd:element ref="cost:WorkTimeTicket" maxOccurs="unbounded"
				minOccurs="0">
			</xsd:element>
		</xsd:sequence>

	</xsd:complexType>
</xsd:schema>

            <xsd:extension base="exception:GeneralWebserviceException"/>
         </xsd:complexContent>
      </xsd:complexType>
   </xsd:element>
   <xsd:element name="AssertionException">
      <xsd:complexType>
         <xsd:complexContent>
            <xsd:extension base="exception:GeneralWebserviceException"/>

         </xsd:complexContent>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="InvalidLicenseException">

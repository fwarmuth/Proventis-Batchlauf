      <xsd:complexType>
         <xsd:complexContent>
            <xsd:extension base="exception:UserManagementException"/>
         </xsd:complexContent>

      </xsd:complexType>
   </xsd:element>
   <xsd:element name="InvalidDatesException">
      <xsd:complexType>
         <xsd:complexContent>
            <xsd:extension base="exception:UserManagementException"/>
         </xsd:complexContent>
      </xsd:complexType>
   </xsd:element>

   <xsd:element name="ServerMaintenanceException">
      <xsd:complexType>
         <xsd:complexContent>
            <xsd:extension base="exception:UserManagementException"/>
         </xsd:complexContent>
      </xsd:complexType>
   </xsd:element>
   <xsd:element name="InvalidParameterException">
      <xsd:complexType>

